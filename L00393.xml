               </listPlace><listPerson><person xml:id="tillich_person_id__2323">
                  <persName>Kluck, Alexander Heinrich Rudolph von</persName>
                  <birth>
                     <date>1846</date>
                  </birth>
                  <death>
                     <date>1934</date>
                  </death>
                  <occupation>Armeeführer;</occupation>
                  <idno type="gnd">https://d-nb.info/gnd/116235489</idno>
                  <note type="bio"><p>Er war in den 1. Weltkrieg verwickelt.</p></note>
