   <text type="postcard">
      <body>
         <div type="writingSession">
            <pb n="1"/>
            <p><foreign xml:lang="fr">Ni à <rs ref="#tillich_place_id__343" type="place">Paris</rs>, ni à <rs ref="#tillich_place_id__271" type="place">Londres</rs>, ni à <rs ref="#tillich_place_id__369" type="place">Rome</rs>, mais à <rs ref="#tillich_place_id__310" type="place">Nancy</rs>! Mes remerciements pour votre
               carte!</foreign></p>
            <closer>
               <salute><foreign xml:lang="fr">Toujours ton</foreign></salute>
               <signed><hi rend="u"><rs ref="#tillich_person_id__2138" type="person">O.
                     Heller</rs></hi></signed>
            </closer>
            <postscript>
               <p>8st. Berufsarbeiter a. D.</p>
               <p>z. Z. Vagabund i. Frankreich.</p>
            </postscript>
         </div>
         <div type="ZweiteHandschrift">
            <p>Dein alter Bekannter</p>
            <closer><signed><unclear>¿¿¿</unclear></signed></closer>
         </div>
      </body>
